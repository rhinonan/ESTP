doctype html
html
  head
  mete(charset="utf-8")
  mete(http-equiv="X-UA-Compatible" content="IE=edge")
  mete(name="viewport" content="width=device-width, initial-scale=1")
  mete(name="description" content="")
  link(href="../css/bootstrap.min.css" rel="stylesheet")
  link(href="../css/sb-admin.css" rel="stylesheet")
  link(href="../css/plugins/morris.css" rel="stylesheet")
  link(href="../font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css")
  title= title
  body
    div#wrapper
      nav.navbar.navbar-inverse.navbar-fixed-top(role="navigation")
        div.navbar-header
          button.navbar-toggle(type="button" data-toggle="collapse" data-target=".navbar-ex1-collapse")
            span.sr-only 隐藏导航
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') 首页
        ul.nav.navbar-right.top-nav
          li.dropdown
            a.dropdown-toggle(href="#" data-toggle="dropdown")
              i.fafa-envelope
              b.caret
        div.collapse.navbar-collapse.navbar-ex1-collapse
          ul.nav.navbar-nav.side-nav
            if nav='debug'
              li.active
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 测试目录
            else 
              li
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 测试目录
            if (nav=='users')
              li.active
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 用户管理
            else 
              li
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 用户管理
            if nav == 'dynamic'
              li.active
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 创业动态管理
            else 
              li
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 创业动态管理
            if nav == 'require'
              li.active
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 项目需求管理
            else 
              li
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 项目需求管理
            if nav == 'activity'
              li.active
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 创业活动管理
            else 
              li
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 创业活动管理
            if nav == 'topic'
              li.active
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 创业话题管理
            else 
              li
                a(href="#")
                  i.glyphicon-music.glyphicon.fa-fw
                  span 创业话题管理

      div#page-wrapper
        .container-fluid
          block content
    scirpt(src="../js/jquery.js")
    scirpt(src="../js/bootstrap.min.js")
    scirpt(src="../js/plugins/morris/raphael.min.js")
    scirpt(src="../js/plugins/morris/morris.min.js")
    scirpt(src="../js/plugins/morris/morris-data.js")
